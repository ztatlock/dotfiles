[user]
  name  = Zach Tatlock
  email = ztatlock@cs.washington.edu

[color]
  ui = auto

[push]
  default = simple

[alias]
  diffw = diff --color-words
  dropbox = !git-dropbox-manage

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[init]
	defaultBranch = main
